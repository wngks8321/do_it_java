package assignment;

public class OperationEx {

	public static void main(String[] args) {
		int age = 24; // 여기서의 "="가 대입연산자임 
		
		System.out.println(age);
		
		int num = 10;
		int num2 = -num; // 부호연산자 "-"
		System.out.println(num);
		System.out.println(num2);
		
		
		System.out.println(5/3);
		System.out.println(5%3);
	}

}
