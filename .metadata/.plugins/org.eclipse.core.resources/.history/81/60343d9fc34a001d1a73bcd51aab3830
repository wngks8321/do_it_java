package binary;

public class ExplicitConversion {

	public static void main(String[] args) {
		int iNum = 1000;
		byte bNum = (byte)iNum;
		
		System.out.println(iNum);
		System.out.println(bNum);
		
		double dNum = 3.14;
		iNum = (int)dNum; //int는 소수점을 표현할 수 없기때문에 3이 됨.
		
		System.out.println(iNum);
	}

}
